<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myapplication1">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="ChainLength"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.SQLiteBike.BikeList">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.ChainLength.PDFActivity"
            android:label="Create PDF"
            android:parentActivityName="com.example.ChainLength.PDFActivity" />
        <activity android:name="com.example.SQLiteBike.AddBike"
            android:label="Add Bike"
            android:parentActivityName="com.example.SQLiteBike.AddBike" />
    </application>

</manifest>